    "@types/lodash": "^4.14.165",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/mkdirp": "^1.0.1",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.12",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/rimraf": "^3.0.0",
    "@types/semver": "^7.3.4",
    "@types/webpack": "^4.41.25",
    "@types/webpack-dev-server": "^3.11.1",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "packi-lint-config": "^0.1.11",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/progress": "^2.0.3",
    "async": "^2.6.3",
    "command-arguments-helper": "^0.1.1",
    "css-loader": "^5.0.1",
    "decompress": "^4.2.1",
    "download": "^8.0.0",
    "ejs": "^3.1.5",
    "eslint-import-resolver-alias": "^1.1.2",
    "execa": "^5.0.0",
    "file-exists": "^5.0.1",
    "file-loader": "^6.2.0",
