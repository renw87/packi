{
  "name": "packi",
  "version": "1.0.9",
  "description": "前端开发工具链.",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "bin": {
    "pki": "bin/index.js",
    "packi": "bin/index.js"
  },
  "types": "lib/",
  "type-check": "tsc",
  "scripts": {
    "test": "nyc mocha --require ts-node/register test/*.spec.ts",
    "coverage": "nyc report --reporter=lcov",
    "es5": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json --watch",
    "prepublishOnly": "rm -rf ./lib && npm run es5",
    "ci": "npm run es5 && npm link && mkdir demo && cd demo && pki init tslib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yusangeng/packi.git"
  },
  "keywords": [
    "toolchain",
    "initialize",
    "scaffold"
  ],
  "author": {
    "name": "yusangeng",
    "email": "yusangeng@outlook.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/download": "^6.2.4",
    "@types/ejs": "^3.0.5",
