    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "package-json-cleanup-loader": "^1.0.3",
    "packagejson": "^1.0.1",
    "packi-print": "^0.1.3",
    "packi-settings": "^0.1.1",
    "postcss": "^8.1.4",
    "postcss-assets": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-px-to-viewport": "^1.1.1",
